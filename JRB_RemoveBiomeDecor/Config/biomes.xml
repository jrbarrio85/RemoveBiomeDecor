<!-- Biome Spawning Decorations -->
	<!-- Scrap Metal piles -->
	<remove xpath="worldgeneration/biomes/biome[contains(@name, '')]/decorations/decoration[@blockname='scrapMetalPile']" />
	<!-- Driftwood  -->
	<remove xpath="worldgeneration/biomes/biome[contains(@name, '')]/decorations/decoration[@blockname='driftwood']" />
	<remove xpath="worldgeneration/biomes/biome[contains(@name, '')]/decorations/decoration[@blockname='driftwood2']" />
	<!-- Cinderblocks -->
	<remove xpath="worldgeneration/biomes/biome[contains(@name, '')]/decorations/decoration[@blockname='cinderBlocks01']" />
	<remove xpath="worldgeneration/biomes/biome[contains(@name, '')]/decorations/decoration[@blockname='cinderBlocks02']" />
	<remove xpath="worldgeneration/biomes/biome[contains(@name, '')]/decorations/decoration[@blockname='cinderBlocks03']" />

</RemoveBiomeDecor>
